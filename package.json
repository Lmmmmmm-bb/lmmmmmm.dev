{
  "name": "vite-vue",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.13.5",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "lint:check": "pnpm prettier --check .",
    "lint:format": "pnpm prettier --write .",
    "lint:fix": "eslint --ext .ts,.js,.vue --fix ./src --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.21",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@unocss/preset-icons": "^0.45.21",
    "@unocss/preset-typography": "^0.45.21",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vueuse/head": "^0.7.11",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.45.21",
    "vite": "^3.0.2",
    "vite-ssg": "^0.20.2",
    "vue-tsc": "^0.38.9"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx,vue}": "eslint --ext .ts,.js,.vue --fix ./src"
  }
}
