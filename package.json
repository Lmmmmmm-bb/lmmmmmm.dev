{
  "name": "lmmmmmm.me",
  "packageManager": "pnpm@7.29.1",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "lint": "pnpm lint:fix & pnpm lint:format",
    "lint:check": "pnpm prettier --check .",
    "lint:format": "pnpm prettier --write .",
    "lint:fix": "eslint --ext .ts,.js,.vue --fix ./src --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.4",
    "@vercel/analytics": "^0.1.11",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@unocss/preset-icons": "^0.50.4",
    "@unocss/preset-typography": "^0.50.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/head": "^1.1.19",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "vite": "^4.1.4",
    "vite-ssg": "^0.22.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx,vue}": "eslint --ext .ts,.js,.vue --fix ./src"
  }
}
